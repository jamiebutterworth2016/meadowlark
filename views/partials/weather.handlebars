<div class="weatherWidget">
    <div>weather partial</div>
    {{#each res.locals.partials.weatherContext}}
    <div>hello</div>
    <div class="location">
        <h3>{{location.name}}</h3>
        <a href="{{forecastUrl}}">
            <img src="{{iconUrl}}" alt="{{weather}}">
            {{weather}},{{temp}}
        </a>
    </div>
    {{/each}}
    <small>Source: <a href="https://www.weather.gov/documentation/services-web-api">National Weather Service</a></small>
</div>